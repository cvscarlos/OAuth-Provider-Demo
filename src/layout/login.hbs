<h3 class="mb-5">Request data:</h3>

<div class="d-inline-block mb-5">
  <table class="table table-responsive table-bordered">
    {{#each queryEntries}}
      <tr>
        <th>{{this.[0]}}</th>
        <td>{{this.[1]}}</td>
      </tr>
    {{/each}}
  </table>
</div>

<button class="btn btn-primary" onclick="location.pathname='/oauth/authorization'">
  Continue
</button>